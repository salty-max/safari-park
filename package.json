    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.1",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.10.2",
    "react-transition-group": "^4.3.0",
    "use-reducer-logger": "^1.0.1"
  },
  "devDependencies": {
    "babel-plugin-react-remove-properties": "^0.3.0",
